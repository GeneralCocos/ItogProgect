{
  "name": "postgres-raw-cbr-connector",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",

    "database.hostname": "postgres-raw",
    "database.port": "5432",
    "database.user": "airflow",
    "database.password": "airflow",
    "database.dbname": "airflow",

    "topic.prefix": "postgres_raw",

    "plugin.name": "pgoutput",
    "slot.name": "debezium_cbr_slot",
    "publication.name": "debezium_cbr_pub",
    "publication.autocreate.mode": "filtered",
    "slot.drop.on.stop": "true",

    "schema.include.list": "raw",
    "table.include.list": "raw.cbr_daily_rates",

    "schema.history.internal.kafka.bootstrap.servers": "kafka-broker-1:29092,kafka-broker-2:29092,kafka-broker-3:29092",
    "schema.history.internal.kafka.topic": "schema-changes.postgres_raw",

    "decimal.handling.mode": "double",
    "time.precision.mode": "adaptive_time_microseconds",
    "tombstones.on.delete": "false",
    "include.schema.changes": "false"
  }
}
